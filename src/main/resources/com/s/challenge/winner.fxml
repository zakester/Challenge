<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.AnchorPane?>
<StackPane style="-fx-background-color: #0C0C0C;" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.s.challenge.WinnerController">

    <VBox VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS">
        <ImageView>
            <Image url="@images/01.png" />
        </ImageView>
    </VBox>
    <VBox alignment="TOP_RIGHT" VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS">
        <ImageView>
            <Image url="@images/02.png" />
        </ImageView>
    </VBox>
    <VBox alignment="BOTTOM_RIGHT" VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS">
        <ImageView>
            <Image url="@images/03.png" />
        </ImageView>
    </VBox>
    <VBox alignment="BOTTOM_LEFT" VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS">
        <ImageView>
            <Image url="@images/04.png" />
        </ImageView>
    </VBox>

    <ImageView>
        <Image url="@images/winner-background.png" />
    </ImageView>

    <Label fx:id="winnerLabel" text="الفوج الأول" stylesheets="@css/winner-label-style.css" />

</StackPane>
